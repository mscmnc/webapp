{
  "name": "web-application",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://mscmnc.github.io/webapp",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.1",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "compile:sass": "node-sass src/assets/sass/main.scss src/assets/css/main.css -w --output-style expanded",
    "prefix": "postcss --use autoprefixer -b \"last 10 versions\" src/assets/css/main.css -o src/assets/css/main.prefix.css",
    "compress:css": "node-sass src/assets/css/main.prefix.css src/assets/css/style.css --output-style compressed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.0.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
